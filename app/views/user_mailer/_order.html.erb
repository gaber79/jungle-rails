
<tr>
  <td>
    <%= image_tag line_item.product.image.tiny %>
  </td>
  <td>
    <h4><%= line_item.product.name %></h4>
  </td>
  <td>
    <%= humanized_money_with_symbol line_item.product.price %>
  </td>
  <td>
    <%= line_item.quantity %>
  </td>
  <td>
    <%= humanized_money_with_symbol Money.new(line_item.total_price_cents) %>
  </td>
</tr>
    
